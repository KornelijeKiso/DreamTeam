<Window x:Class="ProjectTourism.WPF.View.Guest1View.MyProfileWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProjectTourism.WPF.View.Guest1View"
        mc:Ignorable="d"
        Title="MyProfileWindow" Height="750" Width="430">
    <Grid>
        <Image VerticalAlignment="Top" HorizontalAlignment="Center" Source="https://img.icons8.com/cotton/2x/user-male-circle.png" Width="128" Height="128" Margin="0 100 0 0"></Image>
        <Label Content="{Binding Path=Guest1VM.isSuperGuest}" VerticalAlignment="Top" HorizontalAlignment="Center" Width="128" Height="128" Margin="0 300 0 0">
        </Label>
    </Grid>
</Window>
